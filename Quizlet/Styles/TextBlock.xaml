<xaml:ResourceDictionary xmlns:xaml="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="QuizletTourTextHeaderStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="32"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>

    <Style x:Key="QuizletTourTextDescriptionStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="22"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>

    <Style x:Key="QuizletLogoTextStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="70"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontFamily" Value="ms-appx:/Assets/Fonts/Brighton Std Bold.otf#Brighton Std" />
    </Style>

</xaml:ResourceDictionary>